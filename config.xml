<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.coolappz.TapToPublish" version="1.0.0">
    <name>Tap to Publish</name>
    <description>
        
    </description>
    <author email="caballerojavier13@gmail.com" href="http://www.javiercaballero.com.ar">
        Javier Caballero
    </author>
    <content src="index.html"/>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
      <feature name="Notification">
      <param name="ios-package" value="CDVNotification" />
  </feature>
    <access origin="*" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
